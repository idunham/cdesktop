  DROP_ACTION       	ComposeOrOpen
  PUSH_ACTION		Dtmail
  PUSH_RECALL		false
  CLIENT_NAME		dtmail
  HELP_TOPIC		FPOnItemMail
  HELP_VOLUME		FPanel
}



