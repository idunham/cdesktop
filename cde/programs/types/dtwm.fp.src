  PUSH_ACTION		TextEditor
  DROP_ACTION		TextEditor
  HELP_TOPIC		FPOnItemTextEditor
  HELP_VOLUME		FPanel
}


CONTROL Mail
{
  TYPE			icon
  CONTAINER_NAME	Top
